MYSELF=`which "$0" 2>/dev/null`
[ $? -gt 0 -a -f "$0" ] && MYSELF="./$0"

#
# Cleanup paths for Cygwin.
#
case "`uname`" in
CYGWIN*)
	MYSELF=`cygpath --windows --mixed --path "$MYSELF"`
	;;
esac

java=java
if test -n "$JAVA_HOME"
then
	java="$JAVA_HOME/bin/java"
fi

exec "$java" -Djava.awt.headless=true $java_args -jar "$MYSELF" "$@"
exit 1
